public class LocalVariables {
	public void show() {
		int a=10;
		System.out.println("a="+a);
	}
	public void display() {
		int b=20;
		System.out.println("b="+b);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		LocalVariables obj=new LocalVariables();
		obj.show();
        obj.display();
	}

}
